-- Scripts:

local function NCPZCSU_fake_script() -- MainFrame.Selector 
	local script = Instance.new('LocalScript', MainFrame)

	local mainframe = script.Parent
	local selected = nil
	local selector = Instance.new("LocalScript")
	selector.Name = "Selected"
	
	if game.GameId == 424968782 then
		selector.Parent = mainframe.NinjaSimulator
		selected = mainframe.NinjaSimulator
		
	elseif game.GameId == 781563604 then
		selector.Parent = mainframe.DestructionSimulator
		selected = mainframe.DestructionSimulator
		
	elseif game.GameId == 719754874 then
		selector.Parent = mainframe.RagdollEngine
		selected = mainframe.RagdollEngine
	else
		selector.Parent = mainframe.Misc
		selected = mainframe.Misc
	end
	
	
	for i,v in pairs(mainframe:GetChildren()) do
		if v:IsA("TextButton") then
			v.MouseButton1Click:Connect(function()
				if v ~= selected then
					selected.BackgroundColor3 = Color3.new(0,0,0)
					selected = v
					selector.Parent = v
					v.BackgroundColor3 = Color3.fromRGB(60,60,60)				
				end	
			end)
			if v.Name == selector.Parent.Name then
				v.BackgroundColor3 = Color3.fromRGB(60,60,60)
			end
			v.Changed:Connect(function()
				game:GetService("RunService").Stepped:Wait()
				if v.Name == selector.Parent.Name and v.Name == "NinjaSimulator" then
					mainframe.NinjaSimFrame.Visible = true
				elseif v.Name == "NinjaSimulator" then
					mainframe.NinjaSimFrame.Visible = false
				end
				if v.Name == selector.Parent.Name and v.Name == "DestructionSimulator" then
					mainframe.DestructionSim.Visible = true
				elseif v.Name == "DestructionSimulator" then
					mainframe.DestructionSim.Visible = false
				end
				if v.Name == selector.Parent.Name and v.Name == "RagdollEngine" then
					mainframe.RagdollEngFrame.Visible = true
				elseif v.Name == "RagdollEngine" then
					mainframe.RagdollEngFrame.Visible = false
				end
				if v.Name == selector.Parent.Name and v.Name == "Misc" then
					mainframe.MiscFrame.Visible = true
				elseif v.Name == "Misc" then
					mainframe.MiscFrame.Visible = false
				end
			end)
		end
	end
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	local UIS = game:GetService("UserInputService")
	local function dragify(Frame)
		local dragToggle = nil
		local dragSpeed = 1
		local dragInput = nil
		local dragStart = nil
		local dragPos = nil
		function updateInput(input)
			local Delta = input.Position - dragStart
			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				if UIS:GetFocusedTextBox() == nil then
					dragToggle = true
					dragStart = input.Position
					startPos = Frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragToggle = false
						end
					end)
				end
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	dragify(mainframe)
end
coroutine.wrap(NCPZCSU_fake_script)()
local function HTGQ_fake_script() -- DestructionSim.DestructSim 
	local script = Instance.new('LocalScript', DestructionSim)

	local frame = script.Parent
	local as = frame.AutoSell
	local blowuparea = frame.BlowUpArea
	local modrocket = frame.ModRocket
	local modbomb = frame.ModBomb
	local textbox = frame.Parent.TextBox
	local pressed = false
	as.MouseButton1Click:Connect(function()
		if pressed == false then
			pressed = true
			while game:GetService("RunService").Stepped:Wait() do
				local Event = game:GetService("ReplicatedStorage").Remotes.sellBricks
				Event:FireServer()
			end
			as.Text = "Pressed"
		end
	end)
	
	blowuparea.MouseButton1Click:Connect(function()
		local areanumber = textbox.Text
	
		pcall(function()
			game.Players.LocalPlayer.Backpack.Launcher.Parent = game.Players.LocalPlayer.Character
		end)
		local function blowup(part)
			local A_1 = 16087986401691.1961
			local A_2 = game:GetService("Workspace")[game.Players.LocalPlayer.Name].Launcher.Stats
			local A_3 = part.Position
			local A_4 = game:GetService("Workspace")[game.Players.LocalPlayer.Name].Launcher.Assets.Rocket.Boom
			local Event = game:GetService("ReplicatedStorage").Remotes.explodeRocket
			Event:FireServer(A_1, A_2, A_3, A_4)
		end
		for i,v in pairs(workspace.Areas["Area"..areanumber]:GetDescendants()) do
			if v:IsA("BasePart") and v.Anchored == true then
				pcall(function()
					blowup(v)
				end)
				game:GetService("RunService").Stepped:Wait()
			end
		end
	end)
	
	modrocket.MouseButton1Click:Connect(function()
		local rock = game.Players.LocalPlayer.Backpack:FindFirstChild"Launcher" or game.Players.LocalPlayer.Character:FindFirstChild"Launcher"
		if rock then
			rock = rock.Stats
		end
		local modscrip = require(rock)
		modscrip.Cooldown = 0 -- the cooldown duh
		modscrip.RocketSpeed = 5 -- rocket speed
		modscrip.Bursts = 5 -- bursts like a burst rifle
		modscrip.BurstWait = 0 -- cooldown per burst shot
		modscrip.BlastForce = 1000000000 -- idk this
		modscrip.BlastRadius = 500000000 -- idk this
		modscrip.ShotgunAmount = 0 -- spread thing max is 3
	end)
	
	modbomb.MouseButton1Click:Connect(function()
		local bomb = game.Players.LocalPlayer.Backpack:FindFirstChild("Bomb") or game.Players.LocalPlayer.Character:FindFirstChild("Bomb")
		if bomb then
			bomb = bomb.Stats
			bomb = require(bomb)
		end
		bomb.Cooldown = 0
		bomb.BlastRadius = 458309834905
		bomb.BlastForce =  458309834905
	end)
end
coroutine.wrap(HTGQ_fake_script)()
local function AJDT_fake_script() -- RagdollEngFrame.RagdollScript 
	local script = Instance.new('LocalScript', RagdollEngFrame)

	local frame = script.Parent
	local push = frame.Push
	local bomb = frame.Bomb
	local bomball = frame.BombAll
	local antirag = frame.Antirag
	local textbox = frame.Parent.TextBox
	
	
	local function getplayer(plr)
		plr = tostring(plr):lower()
		if plr == "me" or plr == "" then
			return game.Players.LocalPlayer.Name
		end
		for i,v in pairs(game.Players:GetPlayers()) do
			if v.Name:lower():match(plr) then
				return v.Name
			end
		end
	end
	
	
	local pushing = false
	push.MouseButton1Click:Connect(function()
		pushing = not pushing
		if pushing == true then
			push.Text = "Super Push [on]"
		else
			push.Text = "Super Push [off]"
		end
		if pushing == true then
			while pushing == true and game:GetService("RunService").Stepped:Wait() do
				local Event = game.Players.LocalPlayer.Character:FindFirstChild("Push") or game.Players.LocalPlayer.Backpack:FindFirstChild("Push")
				if Event then
					Event = Event.PushEvent
					Event:FireServer()
				end
			end
		end
	end)
	
	local norag = false
	antirag.MouseButton1Click:Connect(function()
		norag = not norag
		if norag == true then
			antirag.Text = "Anti ragdoll [on]"
		else
			antirag.Text = "Anti ragdoll [off]"
		end
		if norag == true then
			local harderpushman = game.Players.LocalPlayer.Character.HumanoidRootPart.ChildAdded:Connect(function(v)
				if v:IsA("BodyForce") then
					game:GetService("RunService").Stepped:Wait()
					v:Destroy()
				end
			end)
			while game:GetService"RunService".Stepped:Wait() and norag == true do
				local bool_1 = false;
				local Target = game:GetService("ReplicatedStorage").LocalRagdollEvent;
				Target:Fire(bool_1);
			end
			harderpushman:Disconnect()
		end
	end)
	
	bomb.MouseButton1Click:Connect(function()
		local whotoattack = getplayer(textbox.Text)
	
	
		local hrp = game.Players[getplayer(whotoattack)].Character:FindFirstChild"UpperTorso" or game.Players[getplayer(whotoattack)].Character:FindFirstChild("Torso") or game.Players[getplayer(whotoattack)].Character.HumanoidRootPart
		local x = hrp.Position.X
		local y = hrp.Position.Y
		local z = hrp.Position.Z
	
		local userdata_1 = Vector3.new(game.Players.LocalPlayer.Character.Head.Position);
		local userdata_2 = CFrame.new(x, y, z);
		local Target = game:GetService("Players").LocalPlayer.Backpack.ImpulseGrenade.CreateGrenade;
		Target:FireServer(userdata_1, userdata_2);
	end)
	
	bomball.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players:GetPlayers()) do
			local whotoattack = getplayer(v.Name)
	
	
			local hrp = game.Players[getplayer(whotoattack)].Character:FindFirstChild"UpperTorso" or game.Players[getplayer(whotoattack)].Character:FindFirstChild("Torso") or game.Players[getplayer(whotoattack)].Character.HumanoidRootPart
			local x = hrp.Position.X
			local y = hrp.Position.Y
			local z = hrp.Position.Z
	
			local userdata_1 = Vector3.new(game.Players.LocalPlayer.Character.Head.Position);
			local userdata_2 = CFrame.new(x, y, z);
			local Target = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild"ImpulseGrenade" or game:GetService("Players").LocalPlayer.Character:FindFirstChild"ImpulseGrenade"
			if Target  then
				Target = Target.CreateGrenade
				Target:FireServer(userdata_1, userdata_2);
			end
		end
	end)
end
coroutine.wrap(AJDT_fake_script)()
local function AADDP_fake_script() -- MiscFrame.MiscScript 
	local script = Instance.new('LocalScript', MiscFrame)

	local function split(text, splitter)
		text = text..splitter
		local returning = {}
		local returningtxt = ""
		for i = 1,#text do
			if text:sub(i,i) ~= splitter and splitter ~= "" then
				returningtxt = returningtxt..text:sub(i,i)
			else
				if splitter == "" then
					returning[#returning+1] = text:sub(i,i)
				else
					returning[#returning+1] = returningtxt
				end
				returningtxt = ""
			end
		end
		return returning
	end
	
	function tablefind(table, string)
		for i,v in pairs(table) do
			if v == string then
				return i
			end
		end
	end
	
	local function getplayer(plr)
		plr = tostring(plr):lower()
		if plr == "me" or plr == "" then
			return game.Players.LocalPlayer.Name
		end
		for i,v in pairs(game.Players:GetPlayers()) do
			if v.Name:lower():match(plr) then
				return v.Name
			end
		end
	end
	
	
	
	local frame = script.Parent
	local textbox = frame.Parent.TextBox
	local antiafk = frame.Afk
	local flybutton = frame.Fly
	local flyspeedbutton = frame.FlySpeedo
	local grav = frame.Gravity
	local jp = frame.JP
	local ws = frame.WalkSpeed
	local noclipitoe = frame.Noclip
	local antilag = frame.AntiLag
	local afkpressed = false
	antiafk.MouseButton1Click:Connect(function()
		afkpressed = not afkpressed
		if afkpressed == true then
			antiafk.Text = "Anti afk [on]"
		else
			antiafk.Text = "Anti afk [off]"
		end
		if afkpressed == true then
			local vu = game:GetService("VirtualUser")
			local idler = game:GetService("Players").LocalPlayer.Idled:connect(function()
				vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
				wait(1)
				vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			end)
			repeat wait() until afkpressed == false
			idler:Disconnect()
			print("Stopped")
		end
	end)
	
	
	
	local maxflyspeed = 2*2
	local flyspeed = maxflyspeed/2
	local flying = false
	local function fly()
		if flying == false then
			flybutton.Text = "Fly [on]"
			spawn(function()
				flying = true
				local x = 0
				local y = 0
				local z = 0
				local keys = {}
				local focusedcheck = ""
				local status = "none"
				game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
				local uis = game:GetService("UserInputService")
				local velocity = Instance.new("BodyVelocity",game.Players.LocalPlayer.Character.HumanoidRootPart)
				velocity.MaxForce = Vector3.new('inf','inf','inf')
				local gyro = Instance.new("BodyGyro",game.Players.LocalPlayer.Character.HumanoidRootPart)
				gyro.MaxTorque = Vector3.new('inf','inf','inf')
				gyro.D = 300
				gyro.P = 9000
				local focuschecc = uis.TextBoxFocused:Connect(function(thing)
					focusedcheck = thing.Name
					uis.TextBoxFocusReleased:Wait()
					focusedcheck = ""
				end)
				velocity.Velocity = Vector3.new(0,0,0)
				local fli = uis.InputBegan:Connect(function(key)
					if not tablefind(keys,uis:GetStringForKeyCode(key.KeyCode)) then
						keys[#keys+1] = uis:GetStringForKeyCode(key.KeyCode)
					end
					if key.KeyCode == Enum.KeyCode.W and focusedcheck == "" then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = game.Players.LocalPlayer.Character.Humanoid.MoveDirection * 16
							local cameray = game.Workspace.Camera.CFrame.LookVector.Y*16
							if tablefind(keys,"S") or tablefind(keys,"Q") or tablefind(keys,'E') then
								if tablefind(keys,"Q") then
									cameray = -16
								elseif tablefind(keys,"E") then
									cameray = 16
								else
									velocity.Velocity = Vector3.new(0,0,0)
									repeat game:GetService("RunService").Stepped:Wait() until not tablefind(keys,"S") and not tablefind(keys,"Q") and not tablefind(keys,'E')
								end
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
	
					if key.KeyCode == Enum.KeyCode.S and focusedcheck == "" then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = game.Players.LocalPlayer.Character.Humanoid.MoveDirection * 16
							local cameray = -(game.Workspace.Camera.CFrame.LookVector.Y*16)
							if tablefind(keys,"W") or tablefind(keys,"Q") or tablefind(keys,'E') then
								if tablefind(keys,"Q") then
									cameray = -16
								elseif tablefind(keys,"E") then
									cameray = 16
								else
									velocity.Velocity = Vector3.new(0,0,0)
									repeat game:GetService("RunService").Stepped:Wait() until not tablefind(keys,"W") and not tablefind(keys,"Q") and not tablefind(keys,'E')
	
								end
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
					if key.KeyCode == Enum.KeyCode.A and focusedcheck == "" or key.KeyCode == Enum.KeyCode.D and focusedcheck == "" then
						local progress = 0
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = game.Players.LocalPlayer.Character.Humanoid.MoveDirection * 16
							local cameray = velocity.Velocity.Y
							direction = Vector3.new(direction.X*flyspeed,cameray,direction.Z*flyspeed)
							velocity.Velocity = direction
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
					if key.KeyCode == Enum.KeyCode.E and  focusedcheck == ""  then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = game.Players.LocalPlayer.Character.Humanoid.MoveDirection * 0
							local cameray = 16
							if tablefind(keys,'Q') then
								cameray = 0
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
					if key.KeyCode == Enum.KeyCode.Q and  focusedcheck == ""  then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = game.Players.LocalPlayer.Character.Humanoid.MoveDirection * 0
							local cameray = -16
							if tablefind(keys,'E') then
								cameray = 0
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
	
					if key.KeyCode == Enum.KeyCode.Space and focusedcheck == "" then
						game.Players.LocalPlayer.Character.Humanoid.Sit = false
					end
	
					if key.KeyCode == Enum.KeyCode.LeftShift and focusedcheck == "" then
						flyspeed = maxflyspeed
						repeat game:GetService("RunService").Stepped:Wait() until key.UserInputState == Enum.UserInputState.End
						flyspeed = maxflyspeed /2
					end
				end)
	
				local qwe = uis.InputEnded:Connect(function(key)
					if tablefind(keys,uis:GetStringForKeyCode(key.KeyCode)) then
						local number = tablefind(keys,uis:GetStringForKeyCode(key.KeyCode))
						table.remove(keys,number)
					end
				end)
	
				while flying == true and game.Players.LocalPlayer.Character.Humanoid.Health > 0 do
					gyro.CFrame = game.Workspace.Camera.CFrame
					game:GetService("RunService").Stepped:Wait()
				end
				fli:Disconnect()
				qwe:Disconnect()
				focuschecc:Disconnect()
				velocity:Destroy()
				gyro:Destroy()
				game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
				flying = false
				flybutton.Text = "Fly [off]"
			end)
		else
			flying = false
			flybutton.Text = "Fly [off]"
		end
	end
	
	function unfly()
		flying = false
	end
	
	function setspeedfly(int)
		maxflyspeed = int*2
		flyspeed = maxflyspeed / 2
	end
	
	flybutton.MouseButton1Click:Connect(function()
		fly()
	end)
	
	jp.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = textbox.Text
	end)
	
	ws.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = textbox.Text
	end)
	
	local noclipping = false
	local function clip()
		noclipping = false
		noclipitoe.Text = "Noclip [off]"
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("BasePart") and not v.Name:match("Hand") and not v.Name:match("Foot") and not v.Name:match("Leg") then
				v.CanCollide = true
			end
		end
	end
	
	local function noclip()
		if noclipping == false then
			noclipping = true
			noclipitoe.Text = "Noclip [on]"
			spawn(function()
				local clipi = game:GetService("RunService").Stepped:Connect(function()
					for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
						if v:IsA("BasePart") and v.CanCollide == true then
							v.CanCollide = false
						end
					end
				end)
				repeat game:GetService("RunService").Stepped:Wait() until noclipping == false or game.Players.LocalPlayer.Character.Humanoid.Health <= 0
				noclipping = false
				clip()
				clipi:Disconnect()
			end)
		else
			clip()
		end
	end
	
	noclipitoe.MouseButton1Click:Connect(function()
		noclip()
	end)
	
	antilag.MouseButton1Click:Connect(function()
		local target = getplayer(textbox.Text)
		if target then
			target = game.Players[target]
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character:WaitForChild"HumanoidRootPart".CFrame - target.Character:WaitForChild"HumanoidRootPart".CFrame.LookVector * 2
		end
	end)
	
	grav.MouseButton1Click:Connect(function()
		workspace.Gravity = textbox.Text
	end)
	
	
	
	flyspeedbutton.MouseButton1Click:Connect(function()
		setspeedfly(textbox.Text)
	end)
end
coroutine.wrap(AADDP_fake_script)()
local function GZAFWBJ_fake_script() -- NinjaSimFrame.NinjaFrameStuff 
	local script = Instance.new('LocalScript', NinjaSimFrame)

	local function split(text, splitter)
		text = text..splitter
		local returning = {}
		local returningtxt = ""
		for i = 1,#text do
			if text:sub(i,i) ~= splitter and splitter ~= "" then
				returningtxt = returningtxt..text:sub(i,i)
			else
				if splitter == "" then
					returning[#returning+1] = text:sub(i,i)
				else
					returning[#returning+1] = returningtxt
				end
				returningtxt = ""
			end
		end
		return returning
	end
	
	local frame = script.Parent
	local tab = script.Parent.Parent.NinjaSimulator
	local antismoke = frame.AntiSmoke
	local killbutton = frame.Kill
	local knockback = frame.Knockback
	local antiknock = frame.AntiKnockback
	local BlockTools = frame.BlockTools
	local reflectdamage = frame.ReflectDamage
	local mute = frame.Mute
	local autofarm = frame.AutoFarm
	local modspeed = frame.ModSwordSpeed
	local modair = frame.ModSwordTime
	local modknock = frame.ModSwordKnockback
	local crash = frame.Crash
	local textbox = frame.Parent.TextBox
	
	local function getplayer(plr)
		plr = tostring(plr):lower()
		if plr == "me" or plr == "" then
			return game.Players.LocalPlayer.Name
		end
		for i,v in pairs(game.Players:GetPlayers()) do
			if v.Name:lower():match(plr) then
				return v.Name
			end
		end
	end
	
	
	local antismokeclicked = false
	antismoke.MouseButton1Click:Connect(function()
		if antismokeclicked == false then
			antismokeclicked = true
			game.Players.LocalPlayer.PlayerGui.ChildAdded:Connect(function(thing)
				game:GetService("RunService").Stepped:Wait()
				if thing.Name == "SmokeScreen" then
					thing:Destroy()
				end
			end)
			antismoke.Text = "Used"
		end
	end)
	
	killbutton.MouseButton1Click:Connect(function()
		local target = getplayer(textbox.Text)
		function damage(plr)
			if plr then
				local A_1 = game.Players[plr].Character.Humanoid
				local A_2 = "spirit"
				local Event = game:GetService("ReplicatedStorage").Remotes.Damage
				Event:InvokeServer(A_1, A_2)
			end
		end
	
		if target then
			repeat
				game:GetService("RunService").Stepped:Wait()
				spawn(function()
					pcall(function()
						damage(target)
					end)
				end)
			until not game.Players:FindFirstChild(target) or game.Players[target].Character.Humanoid.Health <= 0
		end
	end)
	
	antiknock.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.ChildAdded:Connect(function(part)
			game:GetService("RunService").Stepped:Wait()
			if part:IsA("BodyVelocity") then
				part:Destroy()
			end
		end)
	end)
	
	BlockTools.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:FindFirstChild"Handle" then
				for i,l in pairs(v.Handle:GetChildren()) do
					if l:IsA("SpecialMesh") or l:IsA("BlockMesh") or v:IsA("MeshPart") then
						l:Destroy()
					end
				end
			end
		end
	
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:FindFirstChild"Handle" then
				for i,l in pairs(v.Handle:GetChildren()) do
					if l:IsA("SpecialMesh") or l:IsA("BlockMesh") or v:IsA("MeshPart") then
						l:Destroy()
					end
				end
			end
		end
	end)
	
	knockback.MouseButton1Click:Connect(function()
		local args = split(textbox.Text," ")
		local function knockback(plr,int1,int2)
			local A_1 = game.Players[plr].Character.Humanoid
			local A_2 = int1 -- speed
			local A_3 = int2 -- time
			local Event = game:GetService("ReplicatedStorage").Remotes.Knockback
			Event:InvokeServer(A_1, A_2, A_3)
		end
		knockback(getplayer(args[1]), args[2], args[3])
	end)
	
	reflectdamage.MouseButton1Click:Connect(function()
		local target = getplayer(textbox.Text)
		local function knockback(plr,int1,int2)
			local A_1 = game.Players[plr].Character.Humanoid
			local A_2 = int1 -- speed
			local A_3 = int2 -- time
			local Event = game:GetService("ReplicatedStorage").Remotes.Knockback
			Event:InvokeServer(A_1, A_2, A_3)
		end
	
		local function damage(plr)
			local A_1 = game.Players[plr].Character.Humanoid
			local A_2 = "spirit"
			local tool = game.Players[plr].Character:FindFirstChildWhichIsA("Tool")
			if tool then
				if tool.Name == "Frozen Heart" then
					A_2 = "frozen"
				elseif tool.Name == "Neptas" or tool.Name == "Spirt Of Life" or tool.Name == "Bloody Heart" or tool.Name == "Flaming Katana" then
					A_2 = "spirit"
					if tool.Name == "Neptas" then
						knockback(plr, 5234000,2342344)
					elseif tool.Name == "Bloody Heart" then
						knockback(plr, 100,0.2)
					else
						knockback(plr, 50,0.15)
					end
				elseif tool.Name:lower():match("katana") or tool.Name:lower():match("blade") or tool.Name:lower():match("katana") or tool.Name:lower():match("slimy") or tool.Name:lower():match("rainbow") or tool.Name:lower():match("oof") or tool.Name == "X" then
					A_2 = "katana"
				else
					A_2 = "katana"
					knockback(plr, 25,0.2)
				end
			else
				A_2 = "punch"
				knockback(plr, 25,0.2)
			end
			local Event = game:GetService("ReplicatedStorage").Remotes.Damage
			Event:InvokeServer(A_1, A_2)
		end
		local lastname = ""
		local used = false
		game.Players[target].Character.Humanoid.ChildAdded:Connect(function(part)
			if part.Name == "creator" and part:IsA("ObjectValue") and used == false then
				used = true
				damage(part.Value.Name)
			end
			game:GetService("RunService").Stepped:Wait()
			used = false
		end)
	end)
	
	mute.MouseButton1Click:Connect(function()
		local player = getplayer(textbox.Text)
		while game:GetService"RunService".Stepped:Wait() do
			if game.Players:FindFirstChild(player) then
				local fluteie = game.Players[player].Character:FindFirstChild("Flute")
				if fluteie then
					fluteie = fluteie.Handle
					local sound = fluteie:FindFirstChildWhichIsA("Sound")
					if sound then
						sound.Playing = false
					end
				end
			end
		end
	end)
	
	autofarm.MouseButton1Click:Connect(function()
		_G.slowgainchininjasimtaCO = not _G.slowgainchininjasimtaCO
		local levelswanted = math.huge
		local levelsgained = 0
		local currentlevel = game.Players.LocalPlayer.leaderstats.Level.Value
		local poopieheadermo = game.Players.LocalPlayer.leaderstats.Level.Changed:Connect(function()
			if game.Players.LocalPlayer.leaderstats.Level.Value ~= currentlevel then
				levelsgained = levelsgained + 1
				currentlevel = game.Players.LocalPlayer.leaderstats.Level.Value
				if levelsgained >= levelswanted then
					_G.slowgainchininjasimtaCO = false
				end
			end
		end)
		local speed = 10
		local function givechi()
			for i = 1,speed do
				if _G.slowgainchininjasimtaCO == true then
					spawn(function()
						pcall(function()
							if _G.slowgainchininjasimtaCO == true and #game.Players:GetPlayers() < 3 then
								local A_1 = game:GetService("Workspace")["先生"].Humanoid
								local A_2 = "punch"
								local Event = game:GetService("ReplicatedStorage").Remotes.Damage
								Event:InvokeServer(A_1, A_2)
							else
								_G.slowgainchininjasimtaCO = false
							end
						end)
					end)
				end
			end
		end
	
		if _G.slowgainchininjasimtaCO == true then
			while _G.slowgainchininjasimtaCO == true do
				game:GetService("RunService").Stepped:Wait()
				if _G.slowgainchininjasimtaCO == true then
					givechi()
					if _G.slowgainchininjasimtaCO == false then
						poopieheadermo:Disconnect()
						break
					end
				else
					poopieheadermo:Disconnect()
					break
				end
			end
		end
	end)
	
	modair.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v.Configuration.Air_Time.Value = tonumber(textbox.Text)
			end
		end
	
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v.Configuration.Air_Time.Value = tonumber(textbox.Text)
			end
		end
	end)
	
	modknock.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v.Configuration.Knock_Force.Value = tonumber(textbox.Text)
			end
		end
	
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v.Configuration.Knock_Force.Value = tonumber(textbox.Text)
			end
		end
	end)
	
	modspeed.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v.Configuration.Swing_Speed.Value = tonumber(textbox.Text)
			end
		end
	
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v.Configuration.Swing_Speed.Value = tonumber(textbox.Text)
			end
		end
	end)
	
	crash.MouseButton1Click:Connect(function()
		local function fix()
			spawn(function()
				game:GetService("ReplicatedStorage").Remotes.FixChar:FireServer();
			end)
		end
		for i = 1,20 do
			spawn(function()
				while game:GetService"RunService".Stepped:Wait() do
					for i = 1,50 do
						fix()
					end
				end
			end)
		end
	end)
end
coroutine.wrap(GZAFWBJ_fake_script)()
